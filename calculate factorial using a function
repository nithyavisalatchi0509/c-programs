#include <stdio.h>

// Function to calculate factorial
long factorial(int n) {
    long fact = 1;
    for(int i = 1; i <= n; i++)
        fact *= i;
    return fact;
}

int main() {
    int num = 5;  // Directly assign the number
    long result = factorial(num);

    printf("Factorial of %d = %ld\n", num, result);

    return 0;
}
