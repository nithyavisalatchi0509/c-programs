#include <stdio.h>

int main()
{
    int choice;

    printf("Menu\n");
    printf("1. Tea\n2. Coffee\n3. Juice\n4. Water\n");
    printf("Enter your choice: ");
    scanf("%d", &choice);

    switch (choice)
    {
        case 1:
            printf("You selected Tea");
            break;
        case 2:
            printf("You selected Coffee");
            break;
        case 3:
            printf("You selected Juice");
            break;
        case 4:
            printf("You selected Water");
            break;
        default:
            printf("Invalid choice");
    }

    return 0;
}
