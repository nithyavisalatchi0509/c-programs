#include <stdio.h>

int main() {
    int n, i, present = 0;
    int attendance[50];

    printf("Enter number of students: ");
    scanf("%d", &n);

    for(i = 0; i < n; i++) {
        printf("Enter attendance for student %d (1 = Present, 0 = Absent): ", i + 1);
        scanf("%d", &attendance[i]);

        if(attendance[i] == 1)
            present++;
    }

    printf("\nNumber of Present Students = %d\n", present);

    return 0;
}
